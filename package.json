{
  "name": "Ocoder CLI",
  "version": "0.1.0",
  "description": "Ocoder CLI - from-scratch AI coding agent",
  "type": "module",
  "private": true,
  "bin": {
    "ocoder": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "dev": "npm run build && node ./dist/index.js",
    "check": "npm run build && node ./dist/index.js --help"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "typescript": "^5.8.2"
  }
}
